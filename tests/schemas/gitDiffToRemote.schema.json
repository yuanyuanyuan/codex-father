{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "request": {
    "type": "object",
    "properties": {
      "cwd": {
        "type": "string",
        "description": "工作目录路径"
      },
      "repository": {
        "type": "string",
        "description": "仓库路径"
      },
      "diff": {
        "type": "string",
        "description": "差异内容"
      },
      "remote": {
        "type": "string",
        "description": "远程仓库"
      },
      "branch": {
        "type": "string",
        "description": "分支"
      }
    },
    "required": ["cwd"],
    "additionalProperties": false
  },
  "response": {
    "type": "object",
    "properties": {
      "sha": {
        "type": "string"
      },
      "diff": {
        "type": "string"
      }
    },
    "required": ["sha", "diff"]
  }
}
